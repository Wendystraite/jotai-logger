{
  "name": "jotai-logger",
  "version": "1.1.0",
  "description": "Logging utility for jotai",
  "keywords": [
    "jotai",
    "logger",
    "typescript"
  ],
  "homepage": "https://github.com/Wendystraite/jotai-logger",
  "bugs": {
    "url": "https://github.com/Wendystraite/jotai-logger/issues"
  },
  "author": "Wendy Fouchter <wendy.fouchter@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wendystraite/jotai-logger.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "license": "MIT",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.1",
    "@changesets/cli": "^2.29.3",
    "@eslint/js": "^9.26.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@vitest/coverage-v8": "3.1.3",
    "eslint": "^9.26.0",
    "jiti": "^2.4.2",
    "jotai": "^2.12.4",
    "jotai-devtools": "^0.12.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "publint": "^0.3.12",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vitest": "^3.1.3"
  },
  "scripts": {
    "build": "rm -fr dist && tsc --build tsconfig.lib.json",
    "ci": "pnpm run build && pnpm run check-types && pnpm run check-format && pnpm run lint:package && pnpm run check-exports && pnpm run lint && pnpm run test",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "check-types": "tsc --build tsconfig.tests.json --noEmit",
    "lint": "eslint",
    "lint:package": "publint",
    "test": "vitest run",
    "coverage": "vitest run --coverage --coverage.include=src",
    "dev": "vitest",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "@types/react": ">=17.0.0",
    "jotai": ">=2.12.4",
    "jotai-devtools": "^0.12.0",
    "react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "jotai-devtools": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "dependencies": {
    "stacktrace-js": "^2.0.2"
  }
}
